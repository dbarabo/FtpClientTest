<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.barabo.carwash">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="ru.barabo.carwash.main.CarWashApp"
        android:allowBackup="true"
        android:icon="@drawable/ico72"
        android:label="Автомойка ТЦ «Стройматериалы"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ru.barabo.carwash.gui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ru.barabo.carwash.gui.PriceActivity"></activity>
    </application>

</manifest>